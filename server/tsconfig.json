{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noEmit": true,
    "types": ["bun-types"],
    "typeRoots": ["./src/types", "./node_modules/@types"]
  }
}
